    "@storybook/test-runner": "^0.22.0",
    "@svgr/cli": "^8.1.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.13.9",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.8.0",
