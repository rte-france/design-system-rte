{
  "name": "@design-system-rte/react",
  "version": "1.4.0",
  "type": "module",
  "module": "./dist/react-package.js",
  "main": "./dist/react-package.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/react-package.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "generate-icons": "npx @svgr/cli -d ./src/components/icon/generated ../core/assets/icons --no-prettier --template template.js && node ./generateIconMap.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "storybook": "storybook dev -p 7008",
    "build-storybook": "storybook build -o ../design-docs/storybook-static/react",
    "test:ci": "npx playwright install && npx start-server-and-test storybook http://localhost:7008 test",
    "test": "test-storybook --url http://localhost:7008 --failOnConsole --maxWorkers=2 --testTimeout=30000",
    "test:watch": "test-storybook --url http://localhost:7008 --watch",
    "prepublishOnly": "npm run generate-icons && npm run build",
    "generate:component": "node scripts/generate-component.js"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "description": "",
  "dependencies": {
    "@design-system-rte/core": "^1.3.0",
    "clsx": "^2.1.1",
    "sass": "^1.85.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@storybook/addon-a11y": "^8.6.6",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/blocks": "^8.6.6",
    "@storybook/react": "^8.6.6",
    "@storybook/react-vite": "^8.6.6",
    "@storybook/test": "^8.6.6",
    "@storybook/test-runner": "^0.23.0",
    "@svgr/cli": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.13.9",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/browser": "^3.0.7",
    "@vitest/coverage-v8": "^3.0.7",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "react": ">=18.0.0",
    "react-dom": "^18.3.1",
    "sass-embedded": "^1.88.0",
    "storybook": "^8.6.6",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}