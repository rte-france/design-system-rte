{
  "name": "@design-system-rte/react",
  "version": "0.1.1",
  "type": "module",
  "module": "dist/react-package.js",
  "main": "dist/react-package.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "generate-icons": "npx @svgr/cli -d ./src/components/icon/generated ../core/assets/icons --template template.js && npm run lint:fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "storybook": "storybook dev -p 7008",
    "build-storybook": "storybook build -o ../design-docs/storybook-static/react",
    "test:ci": "npm run generate-icons && npx playwright install && npx start-server-and-test storybook http://localhost:7008 test",
    "test": "test-storybook --url http://localhost:7008",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
