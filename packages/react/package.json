    "clsx": "^2.1.1",
    "react": ">=18.0.0",
    "react-dom": "^18.3.1",
    "sass": "^1.85.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.21.0",
    "@storybook/addon-a11y": "^8.6.6",
    "@storybook/addon-essentials": "^8.6.6",
